{
	"name": "go-run",
	"icon": "icon.png",
	"displayName": "go-run",
	"publisher": "TeamGOGOGO",
	"description": "make go files run faster",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.run"
	],
	"main": "./extension.js",
	"contributes": {
		"keybindings":[{
			"command": "extension.run",
			"key": "shift+f1",
			"when": "editorTextFocus"
		}],
		"commands": [
			{
				"command": "extension.run",
				"title": "go run"
			}
		]
	},
	"repository": {
		"url": "https://github.com/AndrewLiuZY/go-run"
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.3",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	}
}
